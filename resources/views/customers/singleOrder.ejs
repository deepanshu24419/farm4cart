<section class="status">
  <div class="container mx-auto">
    <div class="status-box w-2/3 mx-auto">
      <div class="flex items-center justify-between mb-12">
        <h1 class="text-xl rounded-full px-4 bg-white font-bold ">
          Track delivery status
        </h1>
        <h6 class="bg-white py-1  text-green-600 txt-xs  px-4 rounded-full ">
          <%= order._id %>
        </h6>
        <input
          id="hiddenInput"
          type="hidden"
          value="<%= JSON.stringify(order) %>"
        />
      </div>
      <ul>
        <li class="status_line text-xl pb-16 " data-status="order_placed">
          <span>
            Order Placed
          </span>
        </li>
        <li class="status_line text-xl pb-16 " data-status="confirmed">
          <span>
            Order confirmation
          </span>
        </li>
        <!-- <li class="status_line text-xl pb-16" data-status="prepared">
          <span>
            Preparation
          </span>
        </li> -->
        <!-- <li class="status_line text-xl pb-16" data-status="delivered">
          <span>
            Out of delivery
          </span>
        </li> -->
        <li class="status_line text-xl " data-status="complete">
          <span>
            complete
          </span>
        </li>
      </ul>
    </div>
  </div>
</section>
